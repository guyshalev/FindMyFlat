{
"name":"Yad2All",
"description":"Helps finding accessible apartments on Yad2 for people with disabilities, using the power of community.",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["http://www.yad2.co.il/Nadlan/*","https://www.yad2.co.il/Nadlan/*"],
      "js": ["myscript.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
"web_accessible_resources": ["images/*.png", "images/*.jpg"],
"permissions": ["https://*/", "http://*/"]
}
